{
	"name": "mern-scaffold",
	"version": "0.0.0",
	"description": "To get you started!",
	"main": "app.ts",
	"scripts": {
		"server": "nodemon app.ts",
		"front": "npm start --prefix front",
		"dev": "concurrently \"npm run server\" \"npm run front\"",
		"start": "NODE_ENV=production ts-node app.js",
		"build": "tsc -p . && copyfiles -u 1 config/production.json dist/config/",
		"format": "prettier --write 'front/src/**/*.{ts,tsx}'"
	},
